  "annotation_instructions": {
    "multi_label_approach": "Each image should be labeled across ALL applicable categories. Not all categories apply to every image (e.g., shooting_phase depends on timing of photo).",
    "quality_standards": {
      "image_requirements": [
        "Clear view of shooter's full body or relevant body segment",
        "Adequate lighting to see joint angles",
        "Minimal blur (shot in progress acceptable)",
        "Basketball visible when relevant to category",
        "Resolution minimum 640x480 pixels"
      ],
      "annotation_accuracy": [
        "Use angle measurement tools when possible",
        "Compare to reference images of elite shooters",
        "Consider body type and shot type context",
        "Mark uncertain labels for review",
        "Cross-reference multiple categories for consistency"
      ]
    },
    "priority_categories": [
      "shooting_hand_mechanics",
      "elbow_alignment",
      "follow_through",
      "balance_stability",
      "release_point_arc"
